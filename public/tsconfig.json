{
	"compilerOptions": {
		"module": "ES6",
		"target": "es6",
		"lib": ["es2015", "DOM"],
		"moduleResolution": "node",
		"outDir": "js",
		"baseUrl": "ts",
		"paths": {
			"@/*": ["*"]
		},
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"strict": true
	},
	"include": ["ts"]
}
